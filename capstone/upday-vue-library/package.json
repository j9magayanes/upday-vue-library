{
  "name": "upday-vue-library",
  "version": "0.2.48",
  "private": false,
  "description": "Starter template to get up and running quickly with Vue and Storybook",
  "author": "Chromatic <https://chromatic.com/>",
  "scripts": {
    "build": "vite build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "build-storybook": "storybook build",
    "dev": "vite",
    "format": "prettier --write src/",
    "init-msw": "msw init public/",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "jest --setupFiles ./setupFile.js",
    "vue": "3.2.16"
  },
  "dependencies": {
    "upday-web-components": "^1.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-a11y": "^7.6.3",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/addon-storyshots": "^7.6.3",
    "@storybook/blocks": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/testing-vue": "^0.0.4",
    "@storybook/testing-vue3": "^1.0.0",
    "@storybook/vue3": "^7.6.3",
    "@storybook/vue3-vite": "^7.0.6",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/vue": "^6.4.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/server-test-utils": "^1.3.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/vue3-jest": "^27.0.0",
    "babel-jest": "^27.5.1",
    "cypress": "^13.6.0",
    "eslint": "^8.34.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-vue": "^9.18.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^1.2.1",
    "msw-storybook-addon": "^1.8.0",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.6",
    "vite": "^4.1.4",
    "vue": "^3.3.10",
    "vue-jest": "^3.0.7",
    "vue-loader": "^16.8.3"
  },
  "bugs": {
    "url": "https://github.com/chromaui/intro-storybook-vue-template/issues"
  },
  "license": "MIT",
  "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
  "repository": {
    "type": "git",
    "url": "https://github.com/chromaui/intro-storybook-vue-template"
  },
  "testMatch": [
    "**/*.spec.(js|ts|jsx|tsx)"
  ]
}
